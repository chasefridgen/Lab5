<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <TextView
            android:text="My First Fun App"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="152dp"
            android:layout_marginEnd="152dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="153dp" android:layout_marginStart="153dp" android:layout_marginBottom="672dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="40dp"
            app:layout_constraintTop_toTopOf="parent" app:fontFamily="casual" android:textSize="30sp"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:srcCompat="@tools:sample/avatars[8]"
            tools:layout_editor_absoluteY="199dp" tools:layout_editor_absoluteX="129dp" android:id="@+id/imageButton2"
            android:contentDescription="fun button"/>
    <RatingBar
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="472dp"
            tools:layout_editor_absoluteX="89dp" android:id="@+id/ratingBar"/>
    <RadioGroup
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="199dp"
            tools:layout_editor_absoluteX="197dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>